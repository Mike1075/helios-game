{
  "builds": [
    {
      "src": "packages/web/next.config.js",
      "use": "@vercel/next"
    },
    {
      "src": "packages/api/main.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/packages/api/main.py"
    },
    {
      "source": "/(.*)",
      "destination": "/packages/web/$1"
    }
  ]
}
