{
  "buildCommand": "cd packages/web && npm run build",
  "outputDirectory": "packages/web/.next",
  "installCommand": "npm install && cd packages/web && npm install",
  "functions": {
    "packages/api/main.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/packages/api/main.py"
    }
  ]
}