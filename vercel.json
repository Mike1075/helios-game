{
  "framework": "nextjs",
  "buildCommand": "cd packages/web && npm install && npm run build",
  "outputDirectory": "packages/web/.next",
  "installCommand": "npm install --prefix packages/web",
  "functions": {
    "packages/api/main.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/packages/api/main.py"
    }
  ]
}