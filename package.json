{
  "name": "helios-monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:web": "npm run dev --workspace=helios-web",
    "dev:api": "python3 -m uvicorn packages.api.main:app --reload",
    "build": "cd packages/web && npm run build",
    "start": "cd packages/web && npm run start"
  },
  "devDependencies": {
    "vercel": "^34.0.0"
  }
}